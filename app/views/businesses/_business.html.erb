<div class="col-md-2 card py-4 text-center mx-auto normal-fonts" id="<%= dom_id business %>">
  <p>
    <strong>Name:</strong>
    <%= business.name %>
  </p>

  <p>
    <strong>Num of employees:</strong>
    <%= business.num_of_employees %>
  </p>

  <p>
    <strong>Industry:</strong>
    <%= business.industry.name %>
  </p>

  <div class="text-center">
    <%= link_to "View", business, class: "btn btn-primary" %>
  </div>

</div>
