<%= form_with url: scoreboard_summary_goal_path, method: :get do |form| %>
  <%= form.label "Select Quater" %>
  <%= form.collection_select :select_id, @goal.business.goals, :id, :unique_code %>
  <%= form.hidden_field :report_type, value: "goal" %>
  <%= form.submit "Apply", class: "btn btn-success my-2" %>
<% end %>
